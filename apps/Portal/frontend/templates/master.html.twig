<!doctype html>
<html lang="en">
<head>

    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!--Import materialize.css-->
    <link type="text/css" rel="stylesheet" href="/css/materialize.css" media="screen,projection"/>

    <link type="text/css" rel="stylesheet" href="/css/myfinance.css"/>


    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">

    <title>Myfinance</title>

</head>
<body>
{{ include('partials/header.html.twig') }}
<main>
    <br>
    <div class="container z-depth-2">
        <div class="section-header">
            <div class="row valign-wrapper">

                <div class="col s10">
                    <h5 class="left-align"> {% block section_header_title %}{% endblock %}</h5>
                </div>

                <div class="col s2">
                    {% block section_header_action %}{% endblock %}
                </div>
            </div>
        </div>
        <div class="section-body">
            {% block main %}{% endblock %}
        </div>
    </div>

    <br>
</main>
{{ include('partials/footer.html.twig') }}

<!--JavaScript at end of body for optimized loading-->
<script type="text/javascript" src="/js/jquery-3.5.1.js"></script>
<script type="text/javascript" src="/js/materialize.js"></script>

<script type="text/javascript" src="/js/myfinance.js"></script>
</body>
</html>