{
  "name": "myfinance/myfinance",
  "license": "MIT",
  "type": "project",
  "description": "Myfinance - Monorepository",
  "require": {
    "php": "^7.4",
    "ext-amqp": "*",
    "ext-apcu": "*",
    "ext-json": "*",
    "ext-zend-opcache": "*",
    "ext-pdo": "*",
    "symfony/framework-bundle": "^5.1",
    "symfony/messenger": "^5.1",
    "symfony/dotenv": "^5.1",
    "symfony/yaml": "^5.1",
    "symfony/twig-bundle": "^5.1",
    "symfony/validator": "^5.1",
    "lambdish/phunctional": "^2.0",
    "ramsey/uuid": "^3.8",
    "doctrine/dbal": "^2.9",
    "doctrine/orm": "^2.6",
    "ocramius/proxy-manager": "2.8",
    "elasticsearch/elasticsearch": "^7.3",
    "monolog/monolog": "^2.0",
    "symfony/monolog-bundle": "^3.5",
    "firebase/php-jwt": "^5.2",
    "ext-bcmath": "*"
  },
  "require-dev": {
    "ext-xdebug": "*",
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8.2",
    "mockery/mockery": "^1.2",
    "fzaninotto/faker": "^1.8",

    "behat/behat": "3.6.1",
    "friends-of-behat/symfony-extension": "2.1.0",
    "friends-of-behat/mink-extension": "2.4.0",
    "friends-of-behat/mink-browserkit-driver": "1.4.0"


  },
  "autoload": {
    "psr-4": {
      "Myfinance\\Apps\\Portal\\Backend\\": "apps/Portal/backend/src",
      "Myfinance\\Apps\\Portal\\Frontend\\": "apps/Portal/frontend/src",
      "Myfinance\\": [
        "src"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Myfinance\\Tests\\": ["tests/src"]
    }
  }
}
